{
	"name": "Chrome Extension Example",
	"manifest_version": 2,
	"version": "1",

	"description": "Show a dialog when the page is done loading",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},

	// Disable the "web_accessible_resources" error
	//"web_accessible_resources": [ "jquery-2.0.3.min.map" ],

	"content_scripts": [ {

		"js": [ "jquery-1.8.3.min.js", "example.js" ],
		"matches": [ "http://*/*", "https://*/*"]
	} ]
}
